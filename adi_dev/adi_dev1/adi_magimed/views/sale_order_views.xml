<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_order_form_magimed_warehouse" model="ir.ui.view">
        <field name="name">sale.order.form.magimed.warehouse</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="priority">20</field>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='partner_details']" position="inside">
                <field name="warehouse_id" options="{'no_create': True}" groups="stock.group_stock_multi_warehouses"/>
            </xpath>
        </field>
    </record>
</odoo>
